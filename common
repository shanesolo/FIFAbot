
def get_wiki_source(subreddit,wiki_name1):
    """
    Returns content of a wiki page to be used for a post
    :param subreddit: subreddit the wiki is on
    :param wiki_name: name of the wiki example: /r/<subreddit>/wiki/fifabot/<wiki_name>
    :return: content of the wiki page
    """
    try:
        content = subreddit.get_wiki_page('fifabot/%s' % wiki_name1).content_md
        content = content.replace('&lt;','<').replace('&gt;','>')
    except:
        content = "No Wiki Found"
    return content

